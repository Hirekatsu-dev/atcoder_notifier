{
  "manifest_version": 3,
  "name": "atcoder notifier",
  "description": "chrome extension that notifies nearest atcoder contest",
  "version": "1.0",
  "icons": {},
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.display_contests.ts.js.js"
      ],
      "matches": [
        "https://*/*"
      ]
    },
    {
      "js": [
        "assets/content-script-loader.add_button_event.ts.js.js"
      ],
      "matches": [
        "https://atcoder.jp/contests/*"
      ],
      "exclude_matches": [
        "https://atcoder.jp/contests/*/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "**/*",
        "*"
      ],
      "use_dynamic_url": true
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  }
}